@model IList<Benday.EasyAuthDemo.WebUi.Models.PersonEditorViewModel>

@{
    ViewBag.Title = "Index";
}

<table class="table table-striped table-hover">
<thead class="thead-light">
    
    <tr>
		<th>first name</th>
<th>last name</th>
<th>phone number</th>
<th>email address</th>

        <th></th>
    </tr>
</thead>
<tbody>
	@for (int i = 0; i < Model.Count; i++)
    {
        @Html.EditorFor(model => model[i])
    }
</tbody>
</table>
