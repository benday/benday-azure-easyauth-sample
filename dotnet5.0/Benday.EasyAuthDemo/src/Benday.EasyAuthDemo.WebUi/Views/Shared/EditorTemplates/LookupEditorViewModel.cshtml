@model Benday.EasyAuthDemo.WebUi.Models.LookupEditorViewModel
<tr>
@Html.HiddenFor(model => model.Id)
<td>
    @Html.EditorFor(model => model.DisplayOrder)
</td>
<td>
    @Html.EditorFor(model => model.LookupType)
</td>
<td>
    @Html.EditorFor(model => model.LookupKey)
</td>
<td>
    @Html.EditorFor(model => model.LookupValue)
</td>
@Html.HiddenFor(model => model.Status)
@Html.HiddenFor(model => model.CreatedBy)
@Html.HiddenFor(model => model.CreatedDate)
@Html.HiddenFor(model => model.LastModifiedBy)
