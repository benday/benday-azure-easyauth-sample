<td>
            @Html.ActionLink("Edit", "Edit", "Lookup", new { id=Model.Id }) |
            @Html.ActionLink("Details", "Details", "Lookup", new { id=Model.Id }) |
            @Html.ActionLink("Delete", "Delete", "Lookup", new { id=Model.Id })
